{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ReverseProxy": {
        "Clusters": {
            "VideoService": {
                "Destinations": {
                    "destination1": {
                        "Address": "https://localhost:62759/"
                    }
                },
                "HttpClient": {
                    "DangerousAcceptAnyServerCertificate": true
                }
            },
            "StorageService": {
                "Destinations": {
                    "destination1": {
                        "Address": "https://localhost:58394/"
                    }
                },
                "HttpClient": {
                    "DangerousAcceptAnyServerCertificate": true
                }
            }
        },
        "Routes": {
            "VideoRoute": {
                "ClusterId": "VideoService",
                "Match": {
                    "Path": "/api/videos/{**remainder}"
                }
            },
            "StorageRoute": {
                "ClusterId": "StorageService",
                "Match": {
                    "Path": "/api/files/{**remainder}"
                }
            }
        }
    }
}
